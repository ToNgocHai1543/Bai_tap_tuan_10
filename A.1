#include <bits/stdc++.h>
using namespace std;
char * concat(const char* s1, const char* s2)
{
    int l1 = strlen(s1);
    int l2 = strlen(s2);
    int l = l1 + l2;
    char* s = new char[l];
    strcpy(s, s1);
    strcat(s, s2);
    return s;
}
int main()
{
    const char* s1 = "Hello";
    const char* s2 = "Wordl";
    char* res = concat(s1, s2);
    cout << res << endl;
    
}
